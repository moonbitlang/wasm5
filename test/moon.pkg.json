{
  "import": [
    "moonbitlang/async/fs",
    "moonbitlang/async/io",
    "moonbitlang/async/process",
    "moonbitlang/async",
    "moonbitlang/core/encoding/utf8",
    {
      "path": "moonbitlang/core/test",
      "alias": "mbtest"
    },
    "moonbitlang/core/strconv",
    {
      "path": "moonbitlang/wasm5/internal/runtime",
      "alias": "wasm5_runtime"
    },
    {
      "path": "moonbitlang/wasm5/internal/cruntime",
      "alias": "wasm5_cruntime"
    },
    {
      "path": "moonbitlang/wasm5/internal/core",
      "alias": "wasm5_core"
    },
    {
      "path": "moonbitlang/wasm5/internal/parse",
      "alias": "wasm5_parse"
    },
    {
      "path": "moonbitlang/wasm5/internal/validate",
      "alias": "wasm5_validate"
    }
  ],
  "pre-build": [
    {
      "input": ["test_manifest.json", "../scripts/generate_tests.py"],
      "output": ["reference_test.mbt"],
      "command": "python3 scripts/generate_tests.py > $output"
    }
  ]
}
